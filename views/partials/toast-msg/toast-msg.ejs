<!-- start Toast message show dailog  -->
<div class="position-fixed bottom-0 end-0 p-3" style="z-index: 11">
  <div
    class="toast-box toast align-items-center text-white bg-primary"
    role="alert"
    aria-live="assertive"
    aria-atomic="true"
  >
    <div class="d-flex">
      <div class="toast-body">
        <h5 class="toast-txt-msg"></h5>
      </div>
      <button
        type="button"
        class="btn-close btn-close-white me-2 m-auto"
        data-bs-dismiss="toast"
        aria-label="Close"
      ></button>
    </div>
  </div>
</div>
<!-- end Toast message show dailog  -->
